
vector<int> compareTriplets(vector<int> a, vector<int> b) {
    vector<int>result;
int ia=0;
    int ib=0;
    for(int i=0; i<a.size(); i++){
        
    
    
        
    
       if (a[i]>b[i]) {
    ia+=1;
      
       }
       else if (a[i]<b[i]) {
       ib+=1;
       }
      
    }

    
result.push_back(ia);
result.push_back(ib);
    

return result;
}
